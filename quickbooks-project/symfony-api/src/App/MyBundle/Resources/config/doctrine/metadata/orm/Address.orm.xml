<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Address" table="address">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="postalcode" type="integer" column="postalCode" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="line5" type="string" column="line5" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="line4" type="string" column="line4" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="line3" type="string" column="line3" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="line2" type="string" column="line2" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="line1" type="string" column="line1" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countrysubdivisioncode" type="string" column="countrySubDivisionCode" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
